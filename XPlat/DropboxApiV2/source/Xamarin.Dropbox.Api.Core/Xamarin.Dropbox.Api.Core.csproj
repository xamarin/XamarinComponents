<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
		<AutoGenerateBindingRedirects>true</AutoGenerateBindingRedirects>
		<PackageVersion>1.0</PackageVersion>
		<Authors>xamarin</Authors>
		<PackageId>Xamarin.Dropbox.Api.iOS</PackageId>
		<Owners>xamarin</Owners>
		<PackageDescription>Things</PackageDescription>
		<Title>Xamarin.Dropbox.Api.iOS</Title>
		<GeneratePackageOnBuild>false</GeneratePackageOnBuild>
		<AssemblyName>Xamarin.Dropbox.Api.Core</AssemblyName>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <OutputPath>bin\Debug\netstandard2.0</OutputPath>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <OutputPath>bin\Release\netstandard2.0</OutputPath>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.NETCore.Portable.Compatibility" Version="1.0.1" />
    <PackageReference Include="Xamarin.Auth" Version="1.5.0.3" />
    <PackageReference Include="NuGet.Build.Packaging" Version="0.1.276" />
    <PackageReference Include="Dropbox.Api" Version="4.5.3" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Data\" />
    <Folder Include="Enums\" />
    <Folder Include="Helpers\" />
  </ItemGroup>
</Project>
